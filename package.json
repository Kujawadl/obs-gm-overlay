{
  "name": "obs-gm-overlay",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "run-p dev:*",
    "dev:web": "next dev",
    "dev:server": "nodemon --watch 'server/**' --ext 'ts,json' ./server/index.ts",
    "build": "run-s build:*",
    "build:web": "next build",
    "build:server": "tsc --project tsconfig.server.json",
    "start": "run-p start:*",
    "start:web": "next start",
    "start:server": "node .server/index.js",
    "lint": "run-s eslint prettier",
    "lint:fix": "run-s eslint:fix prettier:fix",
    "eslint": "eslint --ext js,jsx,ts,tsx,json .",
    "eslint:fix": "eslint --fix --ext js,jsx,ts,tsx,json .",
    "prettier": "prettier --check .",
    "prettier:fix": "prettier --write .",
    "typecheck:web": "next build",
    "typecheck:server": "tsc --noEmit --project tsconfig.server.json",
    "migrate": "ts-node migrate.ts",
    "generate-types": "graphql-codegen --config codegen.yml"
  },
  "dependencies": {
    "@apollo/client": "^3.6.9",
    "@emotion/react": "^11.9.3",
    "@emotion/styled": "^11.9.3",
    "@graphql-tools/schema": "^8.4.0",
    "@mui/icons-material": "^5.8.4",
    "@mui/material": "^5.8.5",
    "apollo-server-core": "^3.9.0",
    "apollo-server-express": "^3.9.0",
    "express": "^4.18.1",
    "formik": "^2.2.9",
    "graphql": "^16.5.0",
    "graphql-subscriptions": "^2.0.0",
    "graphql-ws": "^5.9.0",
    "next": "12.1.6",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "sharp": "^0.30.7",
    "sqlite": "^4.1.1",
    "sqlite3": "^5.0.8",
    "ws": "^8.8.0",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@graphql-codegen/add": "^3.1.1",
    "@graphql-codegen/cli": "2.6.4",
    "@graphql-codegen/introspection": "2.1.1",
    "@graphql-codegen/typescript": "2.5.1",
    "@graphql-codegen/typescript-operations": "2.4.2",
    "@graphql-codegen/typescript-react-apollo": "3.2.16",
    "@graphql-codegen/typescript-resolvers": "2.6.6",
    "@types/graphql": "^14.5.0",
    "@types/node": "18.0.0",
    "@types/react": "18.0.14",
    "@types/react-dom": "18.0.5",
    "@types/ws": "^8.5.3",
    "@typescript-eslint/parser": "^5.30.5",
    "eslint": "8.18.0",
    "eslint-config-next": "12.1.6",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^3.2.4",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "fast-glob": "^3.2.11",
    "nodemon": "^2.0.18",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.7.1",
    "ts-node": "^10.8.1",
    "typescript": "^4.7.4"
  }
}
