{
	"name": "obs-gm-overlay",
	"version": "0.1.1",
	"private": true,
	"scripts": {
		"dev": "nodemon --exec \"next dev\" --ext ts --watch \"./src/graphql/*\" --watch \"./src/pages/api/*\"",
		"build": "next build",
		"start": "next start",
		"lint": "run-s eslint prettier",
		"lint:fix": "run-s eslint:fix prettier:fix",
		"eslint": "eslint --cache --ext js,jsx,ts,tsx,json .",
		"eslint:fix": "eslint --cache --fix --ext js,jsx,ts,tsx,json .",
		"prettier": "prettier --check .",
		"prettier:fix": "prettier --write .",
		"migrate": "ts-node migrate.ts",
		"generate-types": "graphql-codegen --config codegen.yml",
		"typecheck": "tsc",
		"prepare": "husky install",
		"db-up": "docker-compose -f local-db-compose.yml up -d",
		"db-down": "docker-compose -f local-db-compose.yml down"
	},
	"dependencies": {
		"@apollo/client": "^3.8.8",
		"@apollo/server": "^4.10.0",
		"@as-integrations/next": "^3.0.0",
		"@emotion/react": "^11.11.3",
		"@emotion/styled": "^11.11.0",
		"@graphql-tools/schema": "^10.0.2",
		"@graphql-tools/utils": "^10.0.12",
		"@mui/icons-material": "^5.11.0",
		"@mui/material": "^5.11.5",
		"@next-auth/sequelize-adapter": "^1.0.8",
		"date-fns": "^3.1.0",
		"dotenv": "^16.3.1",
		"formik": "^2.4.5",
		"framer-motion": "^8.5.0",
		"graphql": "^16.8.1",
		"graphql-subscriptions": "^2.0.0",
		"graphql-ws": "^5.14.3",
		"lodash": "^4.17.21",
		"next": "^13.1.2",
		"next-auth": "^4.24.5",
		"pg": "^8.11.3",
		"pg-hstore": "^2.3.4",
		"postgres": "^3.4.3",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"react-use": "^17.4.2",
		"sequelize": "^6.35.2",
		"sharp": "^0.33.1",
		"ws": "^8.16.0",
		"yup": "^1.3.3"
	},
	"devDependencies": {
		"@graphql-codegen/add": "^5.0.0",
		"@graphql-codegen/cli": "^5.0.0",
		"@graphql-codegen/introspection": "^4.0.0",
		"@graphql-codegen/typescript": "^4.0.1",
		"@graphql-codegen/typescript-operations": "^4.0.1",
		"@graphql-codegen/typescript-react-apollo": "^4.1.0",
		"@graphql-codegen/typescript-resolvers": "^4.0.1",
		"@types/graphql": "^14.5.0",
		"@types/humanize-duration": "^3.27.4",
		"@types/lodash": "^4.14.202",
		"@types/node": "^20.10.6",
		"@types/pg": "^8.10.9",
		"@types/react": "^18.2.47",
		"@types/react-dom": "^18.2.18",
		"@types/ws": "^8.5.10",
		"@typescript-eslint/parser": "^6.18.0",
		"eslint": "^8.56.0",
		"eslint-config-next": "^14.0.4",
		"eslint-config-prettier": "^9.1.0",
		"eslint-import-resolver-typescript": "^3.6.1",
		"eslint-plugin-import": "^2.29.1",
		"eslint-plugin-unused-imports": "^3.0.0",
		"fast-glob": "^3.3.2",
		"husky": "^8.0.3",
		"lint-staged": "^15.2.0",
		"nodemon": "^3.0.2",
		"npm-run-all": "^4.1.5",
		"postgres-migrations": "^5.3.0",
		"prettier": "^3.1.1",
		"ts-node": "^10.9.2",
		"typescript": "^5.3.3"
	},
	"lint-staged": {
		"*.{ts,tsx,js,jsx,json}": "eslint --cache --fix",
		"*.{ts,tsx,js,jsx,css,md}": "prettier --write"
	}
}
