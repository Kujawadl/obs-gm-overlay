{
	"name": "obs-gm-overlay",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"dev": "nodemon --exec \"next dev\" --ext ts --watch \"./src/graphql/*\" --watch \"./src/pages/api/*\"",
		"build": "next build",
		"start": "next start",
		"lint": "run-s eslint prettier",
		"lint:fix": "run-s eslint:fix prettier:fix",
		"eslint": "eslint --cache --ext js,jsx,ts,tsx,json .",
		"eslint:fix": "eslint --cache --fix --ext js,jsx,ts,tsx,json .",
		"prettier": "prettier --check .",
		"prettier:fix": "prettier --write .",
		"migrate": "ts-node migrate.ts",
		"generate-types": "graphql-codegen --config codegen.yml",
		"typecheck": "tsc",
		"prepare": "husky install"
	},
	"dependencies": {
		"@apollo/client": "^3.7.4",
		"@apollo/server": "^4.3.0",
		"@as-integrations/next": "^1.2.0",
		"@emotion/react": "^11.10.5",
		"@emotion/styled": "^11.10.5",
		"@graphql-tools/schema": "^9.0.13",
		"@mui/icons-material": "^5.11.0",
		"@mui/material": "^5.11.5",
		"better-sqlite3": "^8.0.1",
		"date-fns": "^2.29.3",
		"formik": "^2.2.9",
		"graphql": "^16.6.0",
		"graphql-subscriptions": "^2.0.0",
		"graphql-ws": "^5.9.0",
		"humanize-duration": "^3.28.0",
		"lodash": "^4.17.21",
		"next": "^13.1.2",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"react-use": "^17.4.0",
		"sharp": "^0.31.3",
		"ws": "^8.8.0",
		"yup": "^0.32.11"
	},
	"devDependencies": {
		"@graphql-codegen/add": "^3.2.3",
		"@graphql-codegen/cli": "^2.16.4",
		"@graphql-codegen/introspection": "^2.2.3",
		"@graphql-codegen/typescript": "^2.8.7",
		"@graphql-codegen/typescript-operations": "^2.5.12",
		"@graphql-codegen/typescript-react-apollo": "^3.3.7",
		"@graphql-codegen/typescript-resolvers": "^2.7.12",
		"@types/better-sqlite3": "^7.6.3",
		"@types/graphql": "^14.5.0",
		"@types/humanize-duration": "^3.27.1",
		"@types/node": "^18.11.18",
		"@types/react": "^18.0.27",
		"@types/react-dom": "^18.0.10",
		"@types/ws": "^8.5.4",
		"@typescript-eslint/parser": "^5.48.2",
		"better-sqlite3-helper": "^3.1.6",
		"eslint": "^8.32.0",
		"eslint-config-next": "^13.1.2",
		"eslint-config-prettier": "^8.6.0",
		"eslint-import-resolver-typescript": "^3.5.3",
		"eslint-plugin-import": "^2.27.5",
		"eslint-plugin-unused-imports": "^2.0.0",
		"fast-glob": "^3.2.12",
		"husky": "^8.0.3",
		"lint-staged": "^13.1.0",
		"nodemon": "^2.0.20",
		"npm-run-all": "^4.1.5",
		"prettier": "^2.8.3",
		"ts-node": "^10.9.1",
		"typescript": "^4.9.4"
	},
	"lint-staged": {
		"*.{ts,tsx,js,jsx,json}": "eslint --cache --fix",
		"*.{ts,tsx,js,jsx,css,md}": "prettier --write"
	}
}
